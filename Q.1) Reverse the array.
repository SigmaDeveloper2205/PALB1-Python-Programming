class Solution:
    def reverseArray(self, arr):
        left = 0
        right = len(arr) - 1

        while left < right:
            arr[left], arr[right] = arr[right], arr[left]
            left += 1
            right -= 1

        return arr


n = int(input("Enter number of elements: "))
arr = list(map(int, input("Enter the elements: ").split()))

obj = Solution()
print("Reversed array:", obj.reverseArray(arr))
